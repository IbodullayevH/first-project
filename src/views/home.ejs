<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- bootstrap css link -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />

    <link rel="stylesheet" href="/styles/home_style.css">
    <title>Home</title>
  </head>
  <body>
<div class="box">
    <headers class="headers">
        <h1>This home</h1>
    </headers>

    <main class="main">
        <% categorys_file.forEach(el => { %>
       <div class="card" style="width: 18rem">
          <img src=<%=el.img %> class="card-img-top" alt=<%=el.category_name %> >
             <div class="card-body">
                 <h5 class="card-title">Name:<%= el.category_name %></h5>
                 <a href="/registr" class="btn btn-success"><%= el.category_name %>larni kor'ish</a>
            </div>
        </div>
    
        <%}) %>
    </main>
</div>

  </body>
</html>
